-- SETTING KEY DI SINI
local MY_KEY = "GanTest" 

local Workspace = game:GetService("Workspace")
local UIS = game:GetService("UserInputService")
local CoreGui = game:GetService("CoreGui")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- GUI LOGIN
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "LoginSystem"
ScreenGui.Parent = CoreGui

local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 300, 0, 200)
Frame.Position = UDim2.new(0.5, -150, 0.5, -100)
Frame.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
Frame.Parent = ScreenGui
Instance.new("UICorner", Frame)

local TextBox = Instance.new("TextBox", Frame)
TextBox.Size = UDim2.new(0.8, 0, 0, 45)
TextBox.Position = UDim2.new(0.1, 0, 0.35, 0)
TextBox.PlaceholderText = "Enter Key..."
TextBox.Text = ""
TextBox.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
TextBox.TextColor3 = Color3.new(1, 1, 1)
Instance.new("UICorner", TextBox)

local TextButton = Instance.new("TextButton", Frame)
TextButton.Size = UDim2.new(0.8, 0, 0, 45)
TextButton.Position = UDim2.new(0.1, 0, 0.65, 0)
TextButton.Text = "LOGIN"
TextButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
TextButton.TextColor3 = Color3.new(1, 1, 1)
Instance.new("UICorner", TextButton)

-- GUI MENU UTAMA (Awalnya Tersembunyi)
local ScreenGui2 = Instance.new("ScreenGui")
ScreenGui2.Name = "MainMenu"
ScreenGui2.Enabled = false
ScreenGui2.Parent = CoreGui

local Frame2 = Instance.new("Frame")
Frame2.Size = UDim2.new(0, 230, 0, 340)
Frame2.Position = UDim2.new(0.3, 0, 0.3, 0)
Frame2.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
Frame2.Active = true
Frame2.Draggable = true -- Biar bisa digeser
Frame2.Parent = ScreenGui2
Instance.new("UICorner", Frame2)
local UIStroke = Instance.new("UIStroke", Frame2)
UIStroke.Color = Color3.fromRGB(0, 255, 150)

-- (Bagian Tombol TP tetap sama seperti sebelumnya)
local TextLabel = Instance.new("TextLabel", Frame2)
TextLabel.Size = UDim2.new(1, 0, 0, 50)
TextLabel.Text = "BRAINROTS V3"
TextLabel.TextColor3 = Color3.new(1, 1, 1)
TextLabel.BackgroundTransparency = 1
TextLabel.Parent = Frame2

local function createButton(txt, pos, color, action)
    local btn = Instance.new("TextButton", Frame2)
    btn.Size = UDim2.new(0.85, 0, 0, 50)
    btn.Position = pos
    btn.Text = txt
    btn.BackgroundColor3 = color
    btn.TextColor3 = Color3.new(1, 1, 1)
    Instance.new("UICorner", btn)
    btn.MouseButton1Click:Connect(action)
end

-- Teleport Celestial
createButton("TP TO CELESTIAL", UDim2.new(0.075, 0, 0.18, 0), Color3.fromRGB(150, 0, 255), function()
    local Char = LocalPlayer.Character
    local Target = Workspace:FindFirstChild("Celestial", true)
    if Char and Target and Target:IsA("BasePart") then
        Char.HumanoidRootPart.CFrame = Target.CFrame
    end
end)

-- Teleport Secret
createButton("TP TO SECRET", UDim2.new(0.075, 0, 0.38, 0), Color3.fromRGB(0, 180, 100), function()
    local Char = LocalPlayer.Character
    local Target = Workspace:FindFirstChild("Secret", true)
    if Char and Target and Target:IsA("BasePart") then
        Char.HumanoidRootPart.CFrame = Target.CFrame
    end
end)

-- LOGIC LOGIN
TextButton.MouseButton1Click:Connect(function()
    if TextBox.Text == MY_KEY then
        ScreenGui:Destroy() -- Hapus menu login
        ScreenGui2.Enabled = true -- Munculkan menu cheat
    else
        TextBox.Text = ""
        TextBox.PlaceholderText = "KEY SALAH!"
        task.wait(1)
        TextBox.PlaceholderText = "Enter Key..."
    end
end)
