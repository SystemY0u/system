local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UIS = game:GetService("UserInputService")
local LocalPlayer = Players.LocalPlayer
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "GlobalWallhopV8"
ScreenGui.ResetOnSpawn = false
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.Parent = PlayerGui
local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 210, 0, 175)
Frame.Position = UDim2.new(0.05, 0, 0.3, 0)
Frame.BackgroundColor3 = Color3.fromRGB(12, 12, 12)
Frame.Visible = true
Frame.Active = true
Frame.Draggable = true
Frame.Parent = ScreenGui
local UICorner = Instance.new("UICorner", Frame)
local TextLabel = Instance.new("TextLabel", Frame)
TextLabel.Size = UDim2.new(1, 0, 0, 35)
TextLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 255)
TextLabel.Text = "wallhop .gg/pGRR7uMkr"
TextLabel.TextColor3 = Color3.new(1, 1, 1)
TextLabel.Font = Enum.Font.GothamBold
TextLabel.TextSize = 12
local UICorner2 = Instance.new("UICorner", TextLabel)
local conn = UIS.InputBegan:Connect(function(input, gameProcessed)
    local IsKeyDown = UIS:IsKeyDown(Enum.KeyCode.LeftControl)
end)
local Connect = LocalPlayer.Chatted:Connect(function(value) end)
local TextButton = Instance.new("TextButton", Frame)
TextButton.Size = UDim2.new(0.9, 0, 0, 35)
TextButton.Position = UDim2.new(0.05, 0, 0.3, 0)
TextButton.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
TextButton.Text = "wallhop: disabled"
TextButton.TextColor3 = Color3.new(1, 1, 1)
TextButton.Font = Enum.Font.GothamBold
TextButton.TextSize = 11
local UICorner3 = Instance.new("UICorner", TextButton)
local conn2 = TextButton.MouseButton1Click:Connect(function()
    TextButton.Text = "wallhop: enabled"
    TextButton.BackgroundColor3 = Color3.fromRGB(0, 150, 80)
end)
local TextButton2 = Instance.new("TextButton", Frame)
TextButton2.Size = UDim2.new(0.9, 0, 0, 35)
TextButton2.Position = UDim2.new(0.05, 0, 0.6, 0)
TextButton2.BackgroundColor3 = Color3.fromRGB(100, 20, 20)
TextButton2.Text = "Del GUI"
TextButton2.TextColor3 = Color3.new(1, 1, 1)
TextButton2.Font = Enum.Font.GothamBold
TextButton2.TextSize = 11
local UICorner4 = Instance.new("UICorner", TextButton2)
local conn3 = TextButton2.MouseButton1Click:Connect(function()
    Frame.Visible = false
end)
local conn4 = RunService.Heartbeat:Connect(function(deltaTime)
    local Humanoid = object:FindFirstChildOfClass("Humanoid")
    local HumanoidRootPart = object:FindFirstChild("HumanoidRootPart")
    Humanoid.UseJumpPower = true
    Humanoid.JumpPower = 50
end)
